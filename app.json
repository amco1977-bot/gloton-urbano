{
  "expo": {
    "name": "gloton-urbano",
    "slug": "gloton-urbano",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sporch77.glotonurbano",
      "config": {
        "googleMapsApiKey": "AIzaSyDjTXD-6b7pzSLNHBrrbDoMEG9RdYWQARw"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Permite que Glotón Urbano use tu ubicación",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Permite que Glotón Urbano use tu ubicación",
        "NSLocationAlwaysUsageDescription": "Permite que Glotón Urbano use tu ubicación"
      }
    },
    "android": {
      "package": "com.sporch77.glotonurbano",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDjTXD-6b7pzSLNHBrrbDoMEG9RdYWQARw"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "googleMapsApiKey": "AIzaSyDjTXD-6b7pzSLNHBrrbDoMEG9RdYWQARw",
      "eas": {
        "projectId": "54d78be9-33cd-4ce7-9757-a3bcaa792442"
      }
    },
    "plugins": [
      [
        "expo-maps",
        {
          "googleMapsApiKey": "AIzaSyDjTXD-6b7pzSLNHBrrbDoMEG9RdYWQARw",
          "requestLocationPermission": true,
          "locationPermission": "Permite que Glotón Urbano use tu ubicación"
        }
      ],
      "expo-asset",
      "expo-secure-store"
    ]
  }
}
